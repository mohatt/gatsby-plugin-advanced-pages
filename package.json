{
  "name": "gatsby-plugin-advanced-pages",
  "version": "4.0.0",
  "description": "A Gatsby plugin that makes it easy to create pages with dynamic features like pagination and custom routing",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "files": [
    "cjs/",
    "esm/",
    "types/",
    "gatsby-node.js"
  ],
  "scripts": {
    "build": "rimraf cjs esm && pnpm build:cjs && pnpm build:esm && pnpm build:fragments",
    "build:cjs": "pnpm build:babel --out-dir cjs",
    "build:esm": "BUILD_ESM=1 pnpm build:babel --out-dir esm",
    "build:fragments": "cp src/components/Pagination.fragment.ts esm/components/Pagination.fragment.js",
    "build:babel": "babel src --ignore '**/__tests__,**/*.test.js,**/*.fragment.ts,**/types.ts' --extensions '.ts,.js,.tsx'",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "release": "semantic-release --no-ci",
    "release:inspect": "semantic-release --dry-run",
    "lint": "standard src",
    "lint:fix": "standard src --fix",
    "prepare": "NODE_ENV=production pnpm build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-component",
    "routing",
    "pagination"
  ],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/gatsby-plugin-advanced-pages.git"
  },
  "homepage": "https://github.com/mohatt/gatsby-plugin-advanced-pages",
  "bugs": "https://github.com/mohatt/gatsby-plugin-advanced-pages/issues",
  "dependencies": {
    "@babel/runtime": "^7.26.9",
    "clsx": "^2.1.1",
    "debug": "^4.4.0",
    "js-yaml": "^3.14.1",
    "lodash": "^4.17.21",
    "path-to-regexp": "^6.3.0",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.9",
    "@babel/eslint-parser": "7.26.8",
    "@types/lodash": "4.17.15",
    "@types/jest": "29.5.14",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.5",
    "@types/reach__router": "1.3.15",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "babel-preset-gatsby-package": "3.14.0",
    "gatsby-plugin-utils": "3.19.0",
    "jest": "29.7.0",
    "mkdirp": "3.0.1",
    "metro-memory-fs": "0.81.1",
    "gatsby": "4.25.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.3.1",
    "rimraf": "6.0.1",
    "semantic-release": "22.0.12",
    "standard": "17.1.2",
    "ts-jest": "29.2.6",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "gatsby": ">=4.4.0",
    "react": ">=18.3.0"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "roots": [
      "<rootDir>/test/",
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/__fixtures__/.*$"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "env": [
      "jest"
    ]
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "renovate": {
    "extends": [
      "config:js-lib"
    ]
  }
}
