{
  "name": "gatsby-plugin-advanced-pages",
  "version": "1.0.0",
  "description": "Gatsby plugin that allows easy creation of pages with dynamic features like pagination and custom routing",
  "main": "index.js",
  "files": [
    "dist",
    "index.js",
    "index.js.flow",
    "index.d.ts",
    "gatsby-node.js"
  ],
  "scripts": {
    "lint": "standard",
    "prebuild": "rimraf dist/*",
    "build": "babel src --out-dir dist --ignore src/**/__tests__,src/**/__mocks__",
    "prepare": "cross-env NODE_ENV=production yarn build",
    "watch": "babel -w src --out-dir dist --ignore src/**/__tests__,src/**/__mocks__",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --watch --runInBand"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-component",
    "routing",
    "pagination"
  ],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/gatsby-plugin-advanced-pages.git"
  },
  "homepage": "https://github.com/mohatt/gatsby-plugin-advanced-pages",
  "bugs": "https://github.com/mohatt/gatsby-plugin-advanced-pages/issues",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "paging-algorithm": "^1.0.1",
    "path-to-regexp": "^3.0.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.2.2",
    "cross-env": "^5.1.4",
    "jest": "^24.8.0",
    "metro-memory-fs": "^0.55.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "rimraf": "^2.6.2",
    "standard": "^13.1.0"
  },
  "peerDependencies": {
    "gatsby": ">=2.7.0",
    "prop-types": ">=15.6.1",
    "react": ">=16.4.2"
  },
  "engines": {
    "node": ">=8.5.0"
  }
}
