{
  "name": "gatsby-plugin-advanced-pages",
  "version": "1.0.0",
  "description": "Gatsby plugin that allows easy creation of pages with dynamic features like pagination and custom routing",
  "main": "index.js",
  "files": [
    "dist",
    "index.js",
    "gatsby-node.js"
  ],
  "scripts": {
    "lint": "standard",
    "prebuild": "rimraf dist/*",
    "build": "babel src --out-dir dist --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir dist --ignore **/__tests__"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-component",
    "routing",
    "pagination"
  ],
  "author": "Mohamed Elkholy <mohatt@pm.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mohatt/gatsby-plugin-advanced-pages.git"
  },
  "homepage": "https://github.com/mohatt/gatsby-plugin-advanced-pages",
  "bugs": "https://github.com/mohatt/gatsby-plugin-advanced-pages/issues",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.6.1",
    "path-to-regexp": "^3.0.0",
    "fs-exists-cached": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.2.2",
    "@testing-library/react": "^8.0.6",
    "standard": "^13.1.0",
    "cross-env": "^5.1.4",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "gatsby": "^2.7.0"
  },
  "engines": {
    "node": ">=8.5.0"
  }
}
